function GetYsisInitials() {
	let firstName = Person.Name.NickName;
	let lastName = Person.Name.FamilyName;
    let ysisInitials = firstName.trim().padEnd(2,'X').substring(0,2) + lastName.trim().padEnd(3,'X').substring(0,3);
    
    //Remove diacritical chars
	ysisInitials = deleteDiacriticalMarks(ysisInitials); 
    return ysisInitials.toUpperCase();
}
GetYsisInitials();